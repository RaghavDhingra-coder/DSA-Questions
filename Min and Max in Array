class Solution {
  public:
    pair<int, int> getMinMax(vector<int> arr) {
        int min=arr[0],max=arr[0];
        for(int i=0;i<arr.size();i++)
        {
            if(arr[i]<min)
                min=arr[i];
            if(arr[i]>max)
                max=arr[i];
        }
        return {min,max};
    }
};
